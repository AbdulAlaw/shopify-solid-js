<style>
  swiper-container {
    width: 1600px;
}
</style>

<section>
  <div id="app-root"></div>
</section>

<swiper-container
  slides-per-view="1"
  speed="200"
  loop="true"
  navigation="true"
  pagination="true"
>
  <swiper-slide>
    <img src="//via.placeholder.com/900x600" width="900" height="600" loading="lazy" class="w-full" alt="alt">
  </swiper-slide>
  <swiper-slide>
    <img src="//via.placeholder.com/900x600" width="900" height="600" loading="lazy" class="w-full" alt="alt">
  </swiper-slide>
</swiper-container>

{% schema %}
{
  "name": "Header",
  "class": "section-header",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading"
    }
  ]
}
{% endschema %}
